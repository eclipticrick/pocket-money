{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "member-access": [true, "no-public"],
        "quotemark": [true, "single"],
        "arrow-parens": false,
        "no-console": false,
        "semicolon": false,
        "object-literal-sort-keys": false,
        "trailing-comma": false,
        "adjacent-overload-signatures": false
    },
    "rulesDirectory": []
}
